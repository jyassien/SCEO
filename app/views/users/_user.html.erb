<div id="<%= dom_id user %>">
  <p>
    <strong>Username:</strong>
    <%= user.username %>
  </p>

  <p>
    <strong>Email:</strong>
    <%= user.email %>
  </p>

  <p>
    <strong>Password:</strong>
    <%= user.password %>
  </p>

  <p>
    <strong>User type:</strong>
    <%= user.user_type %>
  </p>

  <p>
    <strong>College name:</strong>
    <%= user.college_name %>
  </p>

</div>
